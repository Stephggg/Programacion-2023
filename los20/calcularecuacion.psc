Proceso calcularecuacion
	Escribir "calcular el resultado de esta ecuacion (x+2)*(y+2)";
	definir a, b, resultado1, resultado2, resultadoTotal Como entero;
	Escribir "ingresa un numero para x";
	leer a;
	escribir "ingresa un numero para y";
	leer b;
	resultado1 <- a + 2;
	resultado2 <- b + 2;
	resultadoTotal <- resultado1 * resultado2;
	escribir "(x+2)*(y+2) = ", resultadoTotal;
FinProceso
